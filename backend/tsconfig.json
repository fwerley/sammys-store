{
  "compilerOptions": {
    "module": "ES2015",
    "target": "ES5",
    "typeRoots": ["./types", "./node_modules/@types"],
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts",
    "tests/**/*.ts"
, "src/server.ts"  ],
"exclude": ["node_modules", ".vscode"]
}
